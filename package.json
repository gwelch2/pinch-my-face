  "main": "server.js",
  "scripts": {
    "webpack": "webpack-dev-server --progress",
    "client": "NODE_ENV=dev-for-client node server-es5",
    "server": "NODE_ENV=dev-for-server nodemon --exec babel-node server",
    "start": "NODE_ENV=development nodemon --exec babel-node server",
    "build": "NODE_ENV=production node server-es5",
    "default": "NODE_ENV=production node server-es5",

    "preclient": "npm run clean-dist && npm run build-server",
    "preserver": "npm run build-client",
    "prestart": "npm run clean-dist",
    "prebuild": "npm run clean-dist && npm run build-server",
    "predefault": "npm run clean-dist",

    "clean-dist": "rimraf dist",
    "build-client": "NODE_ENV=dev-for-server webpack -p --progress --colors",
    "build-server": "babel server --out-dir server-es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mattcpchen/pinch-my-face.git"
  },
  "engineStrict": true,
  "engines": {
    "node": "~4.3.0",
    "npm": "~2.14.12"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "jimp": "^0.2.27",
    "method-override": "^2.3.6",
    "rxjs": "^5.0.0-rc.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "~6.7.*",
    "babel-loader": "~6.2.*",
    "babel-preset-es2015": "~6.6.*",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0",
    "progress-bar-webpack-plugin": "^1.9.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "~1.12.*",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "~1.14.*",
    "webpack-hot-middleware": "^2.13.1"
  }
}
